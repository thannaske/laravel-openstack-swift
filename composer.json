{
    "name": "mzur/laravel-openstack-swift",
    "description": "OpenStack Swift storage driver for Laravel/Lumen.",
    "type": "library",
    "require": {
        "nimbusoft/flysystem-openstack-swift": "^0.3",
        "biigle/laravel-cached-openstack": "dev-master",
        "illuminate/support": "^5.1|^6.0|^7.0",
        "illuminate/filesystem": "^5.1|^6.0|^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Zurowietz",
            "email": "martin@zurowietz.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mzur\\Filesystem\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thannaske/laravel-cached-openstack.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Mzur\\Filesystem\\SwiftServiceProvider"
            ]
        }
    }
}
